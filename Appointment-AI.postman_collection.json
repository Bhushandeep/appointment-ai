{
    "info": {
        "name": "Appointment AI API",
        "description": "Collection of API requests for testing the Appointment AI backend",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Health Check",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:3000/health",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "health"
                    ]
                }
            }
        },
        {
            "name": "Parse Appointment - Text (Dentist)",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"text\": \"Book dentist appointment tomorrow at 3pm\"\n}"
                },
                "url": {
                    "raw": "http://localhost:3000/v1/appointment/parse",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "v1",
                        "appointment",
                        "parse"
                    ]
                }
            }
        },
        {
            "name": "Parse Appointment - Text (Cardiology)",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"text\": \"Cardiology next Monday at 10:30am\"\n}"
                },
                "url": {
                    "raw": "http://localhost:3000/v1/appointment/parse",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "v1",
                        "appointment",
                        "parse"
                    ]
                }
            }
        },
        {
            "name": "Parse Appointment - Ambiguous Time",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"text\": \"Eye doctor tomorrow morning\"\n}"
                },
                "url": {
                    "raw": "http://localhost:3000/v1/appointment/parse",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "v1",
                        "appointment",
                        "parse"
                    ]
                }
            }
        },
        {
            "name": "Parse Appointment - Image Upload",
            "request": {
                "method": "POST",
                "header": [],
                "body": {
                    "mode": "formdata",
                    "formdata": [
                        {
                            "key": "image",
                            "type": "file",
                            "src": "test-appointment.png"
                        }
                    ]
                },
                "url": {
                    "raw": "http://localhost:3000/v1/appointment/parse",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "v1",
                        "appointment",
                        "parse"
                    ]
                }
            }
        }
    ]
